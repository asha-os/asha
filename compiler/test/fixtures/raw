inductive Nat {
    zero : Nat,
    succ : Nat -> Nat
}

inductive Option (a : Type) {
    none : Option a,
    some : a -> Option a
}

inductive Array (a : Type) : Nat -> Type {
    nil : Array a Nat::zero,
    cons {n : Nat} : a -> Array a n -> Array a (Nat::succ n)
}

alias ArrayTwo (a : Type) = Array a (Nat::succ (Nat::succ Nat::zero))

def head {a : Type} {n : Nat} : Array a (Nat::succ n) -> Option a
    | Array::cons x _ => Option::some x

def head_array_two {a : Type} : ArrayTwo a -> Option a
    | Array::cons x _ => Option::some x