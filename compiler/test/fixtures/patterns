inductive Bool {
    true : Bool,
    false : Bool
}

inductive ScottNat {
    zero : ScottNat,
    succ : ScottNat -> ScottNat
}

inductive ScottList (a : Type) {
    nil : ScottList a,
    cons : a -> ScottList a -> ScottList a
}

def is_zero : ScottNat -> Bool
    | ScottNat::zero => Bool::true
    | ScottNat::succ _ => Bool::false

def funny (first : ScottNat) : ScottNat -> Bool
    | ScottNat::zero => Bool::true
    | ScottNat::succ c => is_zero c

def is_nil {a : Type} : ScottList a -> Bool
    | ScottList::nil => Bool::true
    | ScottList::cons _ _ => Bool::false

def map {a : Type} {b : Type} (f : a -> b) : ScottList a -> ScottList b
    | ScottList::nil => ScottList::nil
    | ScottList::cons x xs => ScottList::cons (f x) (map f xs)