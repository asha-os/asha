record Person {
    name : Str,
    age : Nat,
}

record Expr (a : Type) (n : Nat) {
    value : a,
    size : Fin n
}

def expr_type {a : Type} {n : Nat} : Type = Expr a n

def expr_value {a : Type} {n : Nat} (value : a) (size : Fin n) : Expr = Expr::new a n value size

record FunExpr {a : Type} {n : Nat} {
    value : a,
    size : Fin n
}

def fun_expr_value {a : Type} {n : Nat} (value : a) (size : Fin n) : FunExpr = FunExpr::new value size